{
  "name": "sweepstakes",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "css": "postcss --config postcss.config.js --watch",
    "minify-css": "postcss --use cssnano public/stylesheets/style.css --output public/stylesheets/style.min.css",
    "migrate": "knex migrate:rollback --env development && knex migrate:latest --env development",
    "seed": "knex seed:run --env development",
    "bootstrap": "npm run migrate && npm run seed",
    "prod": "node ./bin/www"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "bookshelf": "^0.12.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "install": "^0.10.2",
    "jade": "^1.11.0",
    "knex": "^0.14.2",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "npm": "^5.6.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "request": "^2.83.0",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "basscss": "^8.0.3",
    "basscss-background-colors": "^2.1.0",
