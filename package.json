{
  "name": "sweepstakes",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "css": "postcss --config postcss.config.js --watch",
    "minify-css": "postcss --use cssnano public/stylesheets/style.css --output public/stylesheets/style.min.css",
    "migrate": "knex migrate:rollback --env development && knex migrate:latest --env development",
    "seed": "knex seed:run --env development",
    "bootstrap": "npm run migrate && npm run seed",
    "prod": "node ./bin/www"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "bookshelf": "^0.9.5",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.13.0",
    "install": "^0.7.3",
    "jade": "~1.11.0",
    "knex": "^0.11.1",
    "method-override": "^2.3.5",
    "morgan": "^1.6.1",
    "npm": "^3.9.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.5.5",
    "request": "^2.72.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "*",
    "basscss": "^8.0.1",
    "basscss-background-colors": "^2.1.0",
    "basscss-border-colors": "^2.1.0",
    "basscss-btn": "^1.1.1",
    "basscss-btn-outline": "^1.1.0",
    "basscss-btn-primary": "^1.1.0",
    "basscss-btn-sizes": "^1.2.0",
    "basscss-colors": "^2.2.0",
    "basscss-darken": "^1.1.0",
    "basscss-forms": "^1.0.0",
    "basscss-lighten": "^1.1.0",
    "css-mixed-properties": "*",
    "cssnano": "*",
    "cssstats": "*",
    "postcss": "*",
    "postcss-calc": "*",
    "postcss-cli": "*",
    "postcss-custom-media": "*",
    "postcss-custom-properties": "*",
    "postcss-discard-comments": "*",
    "postcss-import": "*",
    "postcss-nesting": "*",
    "postcss-reporter": "*"
  },
  "engines": {
    "node": "6.1.0"
  }
}
